{% extends 'base.html.twig' %}

{% block body %}
<div class="row	">
	{% for pro in products %}
	
		<div class="card" style="width: 18rem;">
          
          <div class="card-body">
            <h5 class="card-title">{{ pro.name }}</h5>
            <p class="card-text">{{ pro.description }}</p>
            <a href="#" class="btn btn-primary">{{ pro.version }}</a>
          </div>
        </div>
	
	{% endfor %}
</div>	
{% endblock %}